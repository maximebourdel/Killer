<!DOCTYPE HTML>

<html>
	<head>
	
	    <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
		<script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/jquery-ui.min.js"></script>
		
		{% stylesheets 
			filter='cssrewrite,?yui_css' '@MopaBootstrapBundle/Resources/bootstrap/dist/css/*'%}
			<link href="{{ asset_url }}" type="text/css" rel="stylesheet" media="screen" />
		{% endstylesheets %}
		{% stylesheets 
			filter='cssrewrite,?yui_css' '@MopaBootstrapBundle/Resources/bootstrap/docs/assets/css/*'%}
			<link href="{{ asset_url }}" type="text/css" rel="stylesheet" media="screen" />
		{% endstylesheets %}
		
		
		
		{% javascripts
		    '@MopaBootstrapBundle/Resources/bootstrap/js/tooltip.js'
       		'@MopaBootstrapBundle/Resources/bootstrap/js/*'
   		%}
        	<script src="{{ asset_url }}"></script>
    	{% endjavascripts %}

		
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
		<title>{% block title %}Killer{% endblock %}</title>
		
	</head>
  <body>
  
	  <nav role="navigation" class="navbar navbar-inverse">
		<div class="container-fluid">
	 		<!-- Collect the nav links, forms, and other content for toggling -->
			<div class="collapse navbar-collapse">
				<ul class="nav navbar-nav">	    
				    <li id="index"><a href="{{ path('games_killer_index') }}">Accueil</a></li>
	            	{% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                        <li id="consultListKiller"><a href="{{ path('games_killer_consultListKillers') }}">Voir mes Killers</a></li>
	            	    <li id="createKiller"><a href="{{ path('games_killer_createKiller') }}">Créer un nouveau Killer</a></li>
	            	    <li id="login"><a href="{{ path('fos_user_security_logout') }}">Déconnexion</a></li>
	            	{% else %}
                        <li id="login"><a href="{{ path('fos_user_security_login') }}">Connexion</a></li>
                        <li id="register"><a href="{{ path('fos_user_registration_register') }}">Nouveau?</a></li>
                    {% endif %}
                    
	          	</ul>
	        </div><!-- /.navbar-collapse -->
	 	</div><!-- /.container-fluid -->
	</nav>
  	
    {% block body %}
	    	
    {% endblock %}
    
    {% block script %}
    
    {% endblock %}
    
    <div class="container">
        <br />
        <br />
    	© Maxime Bourdel 2014 Tous droits réservés
    </div>
  
  </body>
</html>


